{
  "name": "leonardo-ai",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "yarn run barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn run test:lint && yarn run test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn run test:unit",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:generate": "npx prisma generate",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix"
  },
  "dependencies": {
    "@prisma/client": "^5.2.0",
    "@tsed/ajv": "^7.35.1",
    "@tsed/common": "^7.35.1",
    "@tsed/core": "^7.35.1",
    "@tsed/di": "^7.35.1",
    "@tsed/engines": "^7.35.1",
    "@tsed/exceptions": "^7.35.1",
    "@tsed/json-mapper": "^7.35.1",
    "@tsed/logger": "^6.6.2",
    "@tsed/logger-file": "^6.6.2",
    "@tsed/openspec": "^7.35.1",
    "@tsed/platform-cache": "^7.35.1",
    "@tsed/platform-exceptions": "^7.35.1",
    "@tsed/platform-express": "^7.35.1",
    "@tsed/platform-log-middleware": "^7.35.1",
    "@tsed/platform-middlewares": "^7.35.1",
    "@tsed/platform-params": "^7.35.1",
    "@tsed/platform-response-filter": "^7.35.1",
    "@tsed/platform-views": "^7.35.1",
    "@tsed/prisma": "^7.35.1",
    "@tsed/schema": "^7.35.1",
    "@tsed/swagger": "^7.35.1",
    "@tsed/typegraphql": "^7.35.1",
    "ajv": "^8.12.0",
    "apollo-datasource": "^3.3.1",
    "apollo-datasource-rest": "^3.5.1",
    "apollo-server-core": "^3.6.2",
    "apollo-server-express": "2.25.2",
    "barrelsby": "^2.8.0",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "dotenv-flow": "^3.3.0",
    "express": "^4.18.2",
    "graphql": "^15.7.2",
    "method-override": "^3.0.0",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "4.6.4",
    "@tsed/cli-plugin-jest": "4.6.4",
    "@tsed/cli-plugin-prisma": "4.6.4",
    "@tsed/cli-plugin-typegraphql": "4.6.4",
    "@types/compression": "^1.7.3",
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
